{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "declaration": false,
        "declarationMap": false,
        "esModuleInterop": true,
        "incremental": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "module": "esnext",
        "moduleResolution": "node",
        "noEmit": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "es5"
    },
    "include": [
        "src"
    ]
}